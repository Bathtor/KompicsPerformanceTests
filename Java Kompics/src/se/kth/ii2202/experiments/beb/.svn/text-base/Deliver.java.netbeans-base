package se.kth.ict.id2203.beb;

import java.io.Serializable;
import se.sics.kompics.Event;
import se.sics.kompics.address.Address;

public class Deliver extends Event implements Serializable {

    protected final Address src;
    
    public Deliver(Address src) {
        this.src = src;
    }

    /**
     * @return the src
     */
    public Address getSrc() {
        return src;
    }

}
